{
	"name": "iswenzz.com-webui",
	"version": "1.3.0",
	"private": true,
	"license": "GPL-3.0+",
	"type": "module",
	"workspaces": [
		"packages/*"
	],
	"scripts": {
		"build": "vite build",
		"lint": "pnpm lint:ts && pnpm lint:scss",
		"lint:scss": "stylelint **/*.{scss,css}",
		"lint:scss:fix": "stylelint --fix **/*.{scss,css}",
		"lint:ts": "eslint **/*.{js,jsx,ts,tsx}",
		"lint:ts:fix": "eslint **/*.{js,jsx,ts,tsx} --fix",
		"prettier": "prettier --write **/*.{js,jsx,ts,tsx,scss}",
		"prettier:check": "prettier --check **/*.{js,jsx,ts,tsx,scss}",
		"start": "vite",
		"test": "jest",
		"test:watch": "jest --watchAll=true"
	},
	"dependencies": {
		"@izui/react": "workspace:*"
	}
}
